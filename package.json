{
  "name": "treact",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@owaiswiz/use-in-view": "^1.0.16",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.5",
    "feather-icons": "^4.28.0",
    "framer-motion": "^4.1.5",
    "react": "^17.0.2",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^17.0.2",
    "react-dotenv": "^0.1.3",
    "react-github-btn": "^1.2.0",
    "react-modal": "^3.13.1",
    "react-rnd": "^10.2.4",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.4.3",
    "react-scripts": "^5.0.0",
    "react-slick": "^0.28.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.2.3",
    "tailwindcss": "^2.1.1",
    "twin.macro": "^2.3.3"
  },
  "scripts": {
    "start": "react-dotenv && react-scripts start",
    "build": " PUBLIC_URL=/webapp/ react-dotenv && react-scripts --max_old_space_size=4096 build",
    "test": "react-dotenv && react-scripts test",
    "eject": "react-dotenv && react-scripts eject",
    "deploy": "react-dotenv && yarn build && netlify deploy --prod --dir=build",
    "copy": "cp -r build/. ../e-ncarte_backend/client/homepage"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "src/**/*.js"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
